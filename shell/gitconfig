[color]
	branch=auto
	diff=auto
	status=auto
  showbranch=auto
	ui=true
[color "branch"]
	current=yellow reverse
	local=yellow
	remote=green
[color "diff"]
	meta=yellow bold
	frag=magenta bold
	old=red bold
	new=green bold
[color "status"]
	added=yellow
	changed=green
	untracked=cyan

[merge]
  tool="vimdiff"
  conflictstyle="diff3"
  prompt=false

[pull]
  rebase=false

[push]
  default=current

[format]
